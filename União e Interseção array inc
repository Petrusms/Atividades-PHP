<?php

$arrayb=array();
$arraya=array();

print("Informe 5 números para o vetor A.\n");
for($i=1; $i<6; $i++){

    $move=readline("Informe a ".$i."°: ");
    array_push($arraya, $move);
}

print("Informe 5 números para o vetor B.\n");
for($i=1; $i<6; $i++){

    $move=readline("Informe a ".$i."°: ");
    array_push($arrayb, $move);
}
