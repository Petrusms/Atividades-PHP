<?php

$numbers = array();

print("Informe 10 números para por no vetor.\n");
for ($i = 1; $i < 11; $i++) {
    $number = readline("Digite o número " . $i . ": ");
    $numbers[] = $number;
}

$min = $numbers[0];
$max = $numbers[0];

foreach ($numbers as $num) {
    if ($num < $min) {
        $min = $num;
    }
    if ($num > $max) {
        $max = $num;
    }
}

print("O maior é $max e o menor é $min")
?>
